<!DOCTYPE html>
<html lang="{{ resources.language_code }}">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{nb_title}}</title>
</head>
<body>
    <p>dummy notebook content</p>
    {%- for cell in nb.cells -%}
        {%- if cell.cell_type == 'markdown'-%}
            {%- set html_value=cell.source | markdown2html | strip_files_prefix -%}
            {{ html_value }}
        {%- elif cell.cell_type == 'code'-%}
            <p>code block</p>
            <pre>{{ cell.source }}</pre>
            {%- if cell.outputs and (not cell.metadata.get("collapsed", False)) -%}
                {%- for output in cell.outputs -%}
                    <p>--> output type {{output.output_type}}</p>


                    {%- if output.output_type in ['execute_result', 'display_data'] -%}
                        {%- block data_priority scoped -%}{%- endblock data_priority -%}
                    {%- elif output.output_type == 'stream' -%}
                        {%- block stream scoped -%}{%- endblock stream -%}
                    {%- elif output.output_type == 'error' -%}
                        <div class="highlight"><pre>
                        {%- for line in output.traceback -%}
                            {%- block traceback_line scoped -%}{%- endblock traceback_line -%}
                        {%- endfor -%}
                        </pre></div>
                    {%- else -%}
                        <p>unknown output type {{output.output_type}}</p>
                    {%- endif -%}


                {%- endfor -%}
            {%- endif -%}
        {%- elif cell.cell_type == 'raw' -%}
            <p>raw block</p>
        {%- else -%}
            <p>unknown cell type {{cell.cell_type}}.</p>
        {%- endif -%}
    {%- endfor -%}
</body>
</html>
